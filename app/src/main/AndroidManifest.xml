        </activity>
        <!-- 显示心电图的活动 -->
        <activity android:name=".ShowHeartWaves" />
        <activity android:name=".InfoActivity" />
        <activity android:name=".InfoActivity$InFoActivityForReport" />
        <activity android:name=".InfoActivityForReport" />
    </application>

</manifest>